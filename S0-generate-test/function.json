{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "Request",
      "methods": [ "get", "post" ],
      "route": "test/generate/{correlationId}"
    },
    {
      "name": "metadataBlob",
      "type": "blob",
      "direction": "out",
      "path": "graphSnapshots/metadata/{correlationId}/metadata.json",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "attachmentBlob",
      "type": "blob",
      "direction": "out",
      "path": "graphSnapshots/attachments/{correlationId}/fake.txt",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
