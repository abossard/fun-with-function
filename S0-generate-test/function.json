{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "Request",
      "methods": [ "get", "post" ],
      "route": "test/generate/{correlationId}"
    },
    {
      "name": "metadataBlob",
      "type": "blob",
      "direction": "out",
      "path": "%METADATA_BLOB_PATH%",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "attachmentBlob",
      "type": "blob",
      "direction": "out",
      "path": "%ATTACHMENT_BLOB_PATH%",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
