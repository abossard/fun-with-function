{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "request",
      "methods": [ "get" ],
      "route": "reports"
    },
    {
      "name": "Response",
      "type": "http",
      "direction": "out"
    },
    {
      "name": "cosmosDoc",
      "type": "cosmosDB",
      "direction": "out",
      "databaseName": "%COSMOS_DATABASE_NAME%",
      "containerName": "mailstats",
      "partitionKey": "/pk",
      "createIfNotExists": true,
      "connection": "CosmosDBConnection"
    }
  ],
  "scriptFile": "run.ps1"
}
