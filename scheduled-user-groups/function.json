{
  "bindings": [
    {
      "name": "Timer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "%USER_GROUPS_TIMER_SCHEDULE%"
    },
    {
      "name": "groupQueue",
      "type": "queue",
      "direction": "out",
      "queueName": "%GRAPH_USER_GROUPS_QUEUE_NAME%",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
