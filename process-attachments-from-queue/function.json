{
  "bindings": [
    {
      "name": "QueueItem",
      "type": "queueTrigger",
      "direction": "in",
      "queueName": "%ATTACHMENTS_QUEUE_NAME%",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "cosmosDoc",
      "type": "cosmosDB",
      "direction": "out",
      "databaseName": "%COSMOS_DATABASE_NAME%",
      "containerName": "%COSMOS_CONTAINER_NAME%",
      "createIfNotExists": true,
      "connection": "CosmosDBConnection"
    }
  ],
  "scriptFile": "run.ps1"
}
