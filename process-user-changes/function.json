{
  "bindings": [
    {
      "name": "QueueItem",
      "type": "queueTrigger",
      "direction": "in",
      "queueName": "%GRAPH_USER_CHANGES_QUEUE_NAME%",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "outputDocument",
      "type": "cosmosDB",
      "direction": "out",
      "databaseName": "%COSMOS_DATABASE_NAME%",
      "containerName": "%COSMOS_CONTAINER_NAME%",
      "connection": "CosmosDBConnection",
      "createIfNotExists": false
    }
  ]
}
