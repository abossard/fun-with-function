{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "Request",
      "methods": [ "get", "post" ],
      "route": "groups/run"
    },
    {
      "name": "groupQueue",
      "type": "queue",
      "direction": "out",
      "queueName": "%GRAPH_USER_GROUPS_QUEUE_NAME%",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "Response",
      "type": "http",
      "direction": "out"
    }
  ],
  "scriptFile": "run.ps1"
}
